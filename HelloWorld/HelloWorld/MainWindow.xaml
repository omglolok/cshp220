<Window x:Class="HelloWorld.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld"
        mc:Ignorable="d"
        Title="MainWindow" Height="377.778" Width="786.111">
    <Grid Margin="10,154,2,108" Background="AntiqueWhite">
        <Button Click="Button_Click" Background="White" Margin="-1,0,1,0">
            <StackPanel Orientation="Horizontal">
                <Image Source="images/ic_thumb_up_black_24dp_2x.png" Height="62" Width="62" />
            </StackPanel>
        </Button>
        <TextBox x:Name="username" HorizontalAlignment="Left" Height="23" Margin="32,-135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="698" TextChanged="username_TextChanged"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Height="23" Margin="32,-107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="698" TextChanged="password_TextChanged"/>
        <Button x:Name="btnSignIn" Content="Sign In" HorizontalAlignment="Left" Margin="33,-79,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>
    </Grid>
</Window>
